--

DROP TABLE IF EXISTS `hms_admin_lab_suppliers_dtls`;
CREATE TABLE `hms_admin_lab_suppliers_dtls` (
  `SUPPLIER_NAME` varchar(75) NOT NULL,
  `ADDRESS_LINE1` varchar(150) DEFAULT NULL,
  `ADDRESS_LINE2` varchar(150) DEFAULT NULL,
  `CITY` varchar(25) NOT NULL,
  `STATE` varchar(25) DEFAULT NULL,
  `PINCODE` varchar(6) DEFAULT NULL,
  `PHONE_NUMBER1` varchar(13) DEFAULT NULL,
  `PHONE_NUMBER2` varchar(13) DEFAULT NULL,
  `MOBILE_NUMBER1` varchar(13) DEFAULT NULL,
  `FAX_NUMBER` varchar(13) DEFAULT NULL,
  `E_MAIL` varchar(50) DEFAULT NULL,
  `DL_NO` varchar(50) DEFAULT NULL,
  `CST` varchar(50) DEFAULT NULL,
  `VAT` varchar(50) DEFAULT NULL,
  `TIN` varchar(50) DEFAULT NULL,
  `STATUS` varchar(10) NOT NULL,
  `ID` int(6) NOT NULL,
  PRIMARY KEY (`SUPPLIER_NAME`),
  UNIQUE KEY `ID` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

