  `PASSWORD` varchar(30) NOT NULL,
  `DEPT_ID` int(6) DEFAULT NULL,
  `ROLENAME` varchar(25) DEFAULT NULL,
  `LOGIN_ID` varchar(10) DEFAULT NULL,
  `EMPTYPE` varchar(25) NOT NULL,
  `EMP_WORK_TYPE` varchar(25) NOT NULL,
  `EMP_SALARY` float(8,2) DEFAULT NULL,
  `EMP_LEAVES` int(6) DEFAULT NULL,
  `ID` int(6) NOT NULL AUTO_INCREMENT,
  PRIMARY KEY (`EMP_ID`),
  UNIQUE KEY `ID` (`ID`),
  KEY `FK_DEPTNAME` (`DEPT_ID`),
  KEY `FK_ROLE` (`ROLENAME`),
  CONSTRAINT `FK_DEPTNAME` FOREIGN KEY (`DEPT_ID`) REFERENCES `hms_admin_department` (`DEPT_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

