DROP TABLE IF EXISTS `hms_admin_lab_test`;
CREATE TABLE `hms_admin_lab_test` (
  `LAB_TEST_CODE` int(25) NOT NULL,
  `LAB_TEST_NAME` varchar(50) NOT NULL,
  `LAB_TEST_GROUP_NAME` varchar(50) DEFAULT NULL,
  `LAB_TEST_TYPE` varchar(30) DEFAULT NULL,
  `LAB_TEST_HRS` varchar(10) DEFAULT NULL,
  `LAB_TEST_DAYS` varchar(10) DEFAULT NULL,
  `LAB_TEST_TIMING` varchar(25) DEFAULT NULL,
  `LAB_TEST_IS_ACTIVE` varchar(10) NOT NULL,
  `ID` int(6) NOT NULL,
  `LAB_TEST_IS_TEMPLATE` varchar(10) NOT NULL,
  `LAB_TEST_METHOD` varchar(100) DEFAULT NULL,
  `LAB_TEST_IS_CULTURE` varchar(10) NOT NULL,
  `DISPLAY_TEST` varchar(10) DEFAULT NULL,
  `REPORTTEST_ID` int(10) DEFAULT NULL,
  `IS_NEW_PAGE` varchar(10) NOT NULL,
  `LAB_TEST_IS_WIDAL` varchar(10) NOT NULL,
  `INTERPRETATION_NOTES` text,
  `LAB_DEPARTMENT` varchar(75) DEFAULT NULL,
  `TEST_SC` varchar(50) NOT NULL,
  `SEND_SMS` varchar(10) NOT NULL,
  PRIMARY KEY (`LAB_TEST_CODE`),
  UNIQUE KEY `ID` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

